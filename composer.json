  "repositories": [
    {
        "url": "https://github.com/jonsuh/hamburgers.git",
        "type": "git"
    }
  ],
  "require": {
    "php": ">=5.3",
    "contao/core-bundle": "~4.4",
    "heimrichhannot/contao-slick": "1.*",
    "erdmannfreunde/euf_grid": "2.*",
    "jonnysp/cookieconsent": "^0.0.8",
    "jonsuh/hamburgers": "1.1.3@dev"
  },
  "require-dev": {
    "erdmannfreunde/euf_nutshell": "~0.2"
