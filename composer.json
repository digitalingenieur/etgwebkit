{
  "name": "etgwebkit/etgwebkit",
  "description": "Simple Webkit for ETG Churches",
  "keywords": ["contao", "etg", "church"],
  "type": "contao-module",
  "homepage": "https://github.com/etgwebkit/etgwebkit",
  "license": "GPL-3.0+",
  "authors":[
    {
      "name":"Samuel Heer | diging.",
      "homepage":"http://diging.de",
      "role":"Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/etgwebkit/contao-slick"
    }
  ],
  "require": {
    "php": ">=5.3",
    "contao/core-bundle": "~4.1",
    "contao-community-alliance/composer-plugin":"3.*",
    "heimrichhannot/contao-slick": "dev-contao4",
    "erdmannfreunde/euf_grid": "2.*"
  },
  "require-dev":{
    "erdmannfreunde/euf_nutshell":"~0.2"
  },
  "extra": {
    "contao": {
      "sources": {
        "dist/TL_ROOT/files/etgwebkit": "files/etgwebkit",
        "dist/TL_ROOT/templates/etgwebkit":"templates/etgwebkit",
        "dist/TL_ROOT/templates/etgwebkit.sql": "templates/etgwebkit.sql"
      }
    }
  }
}
